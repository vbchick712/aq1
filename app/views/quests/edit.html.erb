<h4>Editing Quest</h4>
<%= form_for @quest do |f| %>
	<div class="input-field col s6">
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>

	<div class="input-field col s6">
		<%= f.label :start %>
		<%= f.text_field :start %>
	</div>

	<div class="input-field col s6">
		<%= f.label :description %>
		<%= f.text_field :description %>
	</div>
	<div class="input-field col s6">
		<%= f.label :invite_msg %>
		<%= f.text_field :invite_msg %>
	</div>
	<div class="input-field col s6">
		<%= f.label :start_msg %>
		<%= f.text_field :start_msg %>
	</div>
	<div class="input-field col s6">
		<%= f.label :final_msg %>
		<%= f.text_field :final_msg %>
	</div>

	<%= f.submit class:'btn waves-effect waves-light' %>
	</div>

	<% end %>

<%= link_to 'Show', @quest %> |
<%= link_to 'Back', quests_path %>
