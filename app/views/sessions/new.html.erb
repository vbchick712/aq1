

<h3> Log in </h3>
<%= form_tag sessions_path do %>

<div class="input-field col s6">
	<%= label_tag :name%>
	<%= text_field_tag :name%>
</div>
<div class="input-field col s6">
	<%= label_tag :password %>
	<%= password_field_tag :password %>
</div>
<p>
<%= submit_tag 'Log in' %>
</p>
<% end %>

<%= link_to 'Log Out', logout_path, method: "delete" %>